						SPRINTF str ~%s %x~ ("%str%" offset)
						PATCH_PRINT ~%str%~
					END
				END

			END

			SET cre_check += 1
			SET checked = 1

		END ELSE BEGIN
			PATCH_WARN ~FJ_CRE_VALIDITY returned %SOURCE_FILESPEC% as not valid file~
		END
	END

