DEFINE_PATCH_MACRO ~BAF_splRes_replace~ BEGIN
	PHP_EACH remapped_splRes AS source => dest BEGIN
		REPLACE_EVALUATE CASE_INSENSITIVE ~\([A-Za-z]+Spell\)(%source%)~ BEGIN SET updated = 1 PATCH_PRINT ~Patching %SOURCE_FILESPEC%: %source% => %dest% - %MATCH1% -> %MATCH1%RES~ END ~\1RES("#d#%dest%")~ //HaveSpell, RemoveSpell
		REPLACE_EVALUATE CASE_INSENSITIVE ~SpellCast(\([^,]+\),%source%)~ BEGIN SET updated = 1 PATCH_PRINT ~Patching %SOURCE_FILESPEC%: %source% => %dest% - SpellCast -> SpellCastRES~ END ~SpellCastRES("#d#%dest%",\1)~
		REPLACE_EVALUATE CASE_INSENSITIVE ~SpellCastOnMe(\([^,]+\),%source%)~ BEGIN SET updated = 1 PATCH_PRINT ~Patching %SOURCE_FILESPEC%: %source% => %dest% - SpellCastOnMe -> SpellCastOnMeRES~ END ~SpellCastOnMeRES("#d#%dest%",\1)~
		REPLACE_EVALUATE CASE_INSENSITIVE ~SpellCastPriest(\([^,]+\),%source%)~ BEGIN SET updated = 1 PATCH_PRINT ~Patching %SOURCE_FILESPEC%: %source% => %dest% - SpellCastPriest -> SpellCastPriestRES~ END ~SpellCastPriestRES("#d#%dest%",\1)~
		REPLACE_EVALUATE CASE_INSENSITIVE ~SpellCastInnate(\([^,]+\),%source%)~ BEGIN SET updated = 1 PATCH_PRINT ~Patching %SOURCE_FILESPEC%: %source% => %dest% - SpellCastInnate -> SpellCastInnateRES~ END ~SpellCastInnateRES("#d#%dest%",\1)~
		REPLACE_EVALUATE CASE_INSENSITIVE ~HaveSpellParty(%source%)~ BEGIN PATCH_WARN ~Can't replace %source% => %dest%. There is no HaveSpellParty equivalent trigger that accepts RES reference. %SOURCE_FILESPEC% needs manual changes with HaveSpellRES and NextTriggerObject.~ END ~HaveSpellParty("%source%")~
		REPLACE_EVALUATE CASE_INSENSITIVE ~\([A-Za-z]*Spell\)(\([^,]+\),%source%)~ BEGIN SET updated = 1 PATCH_PRINT ~Patching %SOURCE_FILESPEC%: %source% => %dest% - %MATCH1% -> %MATCH1%RES~ END ~\1RES("#d#%dest%",\2)~ //Spell, ForceSpell, ApplySpell, ReallyForceSpell
		REPLACE_EVALUATE CASE_INSENSITIVE ~\([A-Za-z]*SpellPoint\)(\([^,]+\),%source%)~ BEGIN SET updated = 1 PATCH_PRINT ~Patching %SOURCE_FILESPEC%: %source% => %dest% - %MATCH1% -> %MATCH1%RES~ END ~\1RES("#d#%dest%",\2)~ //SpellPoint, ForceSpellPoint, ReallyForceSpellPoint
		REPLACE_EVALUATE CASE_INSENSITIVE ~\([A-Za-z]+NoDec\)(\([^,]+\),%source%)~ BEGIN SET updated = 1 PATCH_PRINT ~Patching %SOURCE_FILESPEC%: %source% => %dest% - %MATCH1% -> %MATCH1%RES~ END ~\1RES("#d#%dest%",\2)~ //SpellNoDec, SpellPointNoDec
		REPLACE_EVALUATE CASE_INSENSITIVE ~ReallyForceSpellDead(\([^,]+\),%source%)~ BEGIN SET updated = 1 PATCH_PRINT ~Patching %SOURCE_FILESPEC%: %source% => %dest% - ReallyForceSpellDead -> ReallyForceSpellDeadRES~ END ~ReallyForceSpellDeadRES("#d#%dest%",\1)~
		REPLACE_EVALUATE CASE_INSENSITIVE ~ForceSpellRange(\([^,]+\),%source%)~ BEGIN SET updated = 1 PATCH_PRINT ~Patching %SOURCE_FILESPEC%: %source% => %dest% - ForceSpellRange -> ForceSpellRangeRES~ END ~ForceSpellRangeRES("#d#%dest%",\1)~
		REPLACE_EVALUATE CASE_INSENSITIVE ~ForceSpellPointRange(\([^,]+\),%source%)~ BEGIN SET updated = 1 PATCH_PRINT ~Patching %SOURCE_FILESPEC%: %source% => %dest% - ForceSpellPointRange -> ForceSpellPointRangeRES~ END ~ForceSpellPointRangeRES("#d#%dest%",\1)~
	END
END

