DEFINE_PATCH_MACRO ~read_trans_array_from_2da~ BEGIN
	LOCAL_SPRINT fl#r2en_trans ""
	LOCAL_SET i = 0
	LOCAL_SPRINT id ""
	LOCAL_SPRINT string ""

	PATCH_PRINT ~read_trans_array_from_2da~
	READ_2DA_ENTRIES_NOW fl#r2en_trans 3
