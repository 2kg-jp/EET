ACTION_BASH_FOR ~EET/base/2da~ ~.*\.2DA$~ BEGIN
	ACTION_IF NOT FILE_EXISTS_IN_GAME ~%BASH_FOR_FILE%~ BEGIN
		COPY ~%BASH_FOR_FILESPEC%~ ~override~
	END
END

