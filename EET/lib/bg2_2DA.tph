ACTION_DEFINE_ASSOCIATIVE_ARRAY "table_2DA_oldNameNewName2" BEGIN
	"BC1" => "BC3"
	"BC2" => "BC4"
	"BD1" => "BD4"
	"BD2" => "BD5"
	"BF1" => "BF3"
