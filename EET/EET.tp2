		END ELSE PATCH_IF ("%filename%" STRING_EQUAL_CASE "RNDTREAS") BEGIN
			SPRINT item ~RNDTRE~
		END ELSE PATCH_IF ("%filename%" STRING_EQUAL_CASE "RNDWEP") BEGIN
			SPRINT item ~RNDWEP~
		END
