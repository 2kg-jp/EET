		SET_2DA_ENTRY cnt 0 1 ~%item%%value%_~
		SET resref_itm_total = resref_itm_total + 1
		SPRINT EVAL "resref_itm_source%resref_itm_total%" "%item%0%value%"
		SPRINT EVAL "resref_itm_dest%resref_itm_total%" "%item%%value%_"
		INNER_ACTION BEGIN
